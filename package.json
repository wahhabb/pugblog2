{
	"name": "lj-sveltekit-ts-tailwind-pug-starter",
	"description": "A Svelte, Typescript, Tailwindcss, & Pug starter template",
	"homepage": "https://skinnypug.dev",
	"keywords": [
		"Svelte",
		"Typescript",
		"TailwindCSS",
		"Pug",
		"Starter"
	],
	"license": "MIT",
	"version": "0.0.3",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/lightning-jar/svelte-typescript-tailwind-pug-starter"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"engines": {
		"node": ">= 16.0.0",
		"pnpm": ">= 7.0.0",
		"yarn": ">= 1.22.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.25.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@tailwindcss/typography": "^0.5.7",
		"@tailwindcss/forms": "^0.5.3",
		"@typescript-eslint/eslint-plugin": "^5.37",
		"@typescript-eslint/parser": "^5.37.0",
		"autoprefixer": "^10.4.12",
		"eslint": "^8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.16",
		"prettier": "^2.7.1",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"pug": "^3.0.2",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.4.0",
		"typescript": "^4.8.4",
		"vite": "^3.1.4"
	},
	"type": "module"
}
